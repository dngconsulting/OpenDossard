{
  "name": "OpenDossard",
  "version": "0.1.0",
  "description": "Outil de gestion en ligne de resultats sportifs",
  "author": "Open Dossard",
  "license": "MIT",
  "scripts": {
    "prebuild": "node ./node_modules/rimraf dist",
    "build": "node ./node_modules/@nestjs/cli/bin/nest.js build --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node ./node_modules/@nestjs/cli/bin/nest.js start",
    "start:dev": "node ./node_modules/@nestjs/cli/bin/nest.js start --watch",
    "start:debug": "node ./node_modules/@nestjs/cli/bin/nest.js start --debug 0.0.0.0 --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json"
  },
  "dependencies": {
    "@nest-middlewares/compression": "6.0.0",
    "@nest-middlewares/connect-rid": "6.0.0",
    "@nest-middlewares/connect-timeout": "6.0.0",
    "@nest-middlewares/cookie-parser": "6.0.0",
    "@nest-middlewares/errorhandler": "6.0.0",
    "@nest-middlewares/express-bearer-token": "6.0.0",
    "@nest-middlewares/helmet": "6.0.0",
    "@nest-middlewares/passport": "6.0.0",
    "@nest-middlewares/response-time": "6.0.0",
    "@nest-middlewares/serve-favicon": "6.0.1",
    "@nest-middlewares/serve-static": "6.0.0",
    "@nest-middlewares/vhost": "6.0.0",
    "@nestjs/common": "6.10.7",
    "@nestjs/core": "6.10.7",
    "@nestjs/jwt": "6.1.1",
    "@nestjs/passport": "6.1.1",
    "@nestjs/platform-express": "6.10.7",
    "@nestjs/serve-static": "1.0.2",
    "@nestjs/swagger": "4.5.12",
    "@nestjs/typeorm": "6.2.0",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.2.3",
    "class-validator": "0.10.1",
    "convict": "5.1.0",
    "lodash": "4.17.15",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "7.12.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.0",
    "rxjs": "6.5.3",
    "swagger-stats": "^0.95.17",
    "swagger-ui-express": "4.1.4",
    "typeorm": "0.2.19"
  },
  "devDependencies": {
    "@nestjs/cli": "6.12.9",
    "@nestjs/schematics": "6.7.5",
    "@nestjs/testing": "6.10.7",
    "@types/express": "4.17.2",
    "@types/jest": "^24.0.18",
    "@types/node": "12.7.5",
    "@types/passport-jwt": "3.0.3",
    "@types/passport-local": "1.0.33",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "3.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
